{
  "name": "grocery-compare-backend",
  "version": "1.0.0",
  "description": "Backend API for GroceryCompare Poland - Intelligent grocery price comparison platform",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node src/production-server.js",
