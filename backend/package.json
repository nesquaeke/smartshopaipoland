    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "openai": "^5.0.1",
    "pg": "^8.16.0",
    "puppeteer": "^24.9.0",
    "redis": "^5.5.6",
    "winston": "^3.17.0"
